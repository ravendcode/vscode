{
  /*
	 // Place your snippets for HTML here. Each snippet is defined under a snippet name and has a prefix, body and
	 // description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	 // $1, $2 for tab stops, ${id} and ${id:label} and ${1:label} for variables. Variables with the same id are connected.
	 // Example:
	 "Print to console": {
		"prefix": "log",
		"body": [
			"console.log('$1');",
			"$2"
		],
		"description": "Log output to console"
	}
*/
  "{{}}": {
    "prefix": "nn",
    "body": [
      "{{$1}}$2"
    ],
    "description": "ng print"
  },
  "html5": {
    "prefix": "html5",
    "body": [
      "<!DOCTYPE html>",
      "<html lang=\"ru\">",
      "<head>",
      "\t<meta charset=\"utf-8\">",
      "\t<meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">",
      "\t<meta name=\"viewport\" content=\"width=device-width, initial-scale=1, shrink-to-fit=no\">",
      "\t<meta name=\"description\" content=\"\">",
      "\t<meta name=\"keywords\" content=\"\">",
      "\t<meta name=\"robots\" content=\"all\">",
      "\t<title>$1</title>",
      "</head>",
      "<body>",
      "\t$2",
      "</body>",
      "</html>"
    ],
    "description": "html5"
  },
  "jinja {{}}": {
    "prefix": "jj",
    "body": [
      "{{ $1 }}"
    ],
    "description": "jinja {{}}"
  },
  "jinja %": {
    "prefix": "ji",
    "body": [
      "{% $1 %}"
    ],
    "description": "jinja %"
  },
  "jinja if": {
    "prefix": "jif",
    "body": [
      "{% if $1 %}",
      "\t$2",
      "{% endif %}"
    ],
    "description": "jinja if"
  },
  "jinja title": {
    "prefix": "jt",
    "body": [
      "{% block title %}$1{% endblock %}"
    ],
    "description": "jinja title"
  },
  "jinja block": {
    "prefix": "jb",
    "body": [
      "{% block ${1:content} %}",
      "\t$2",
      "{% endblock %}"
    ],
    "description": "jinja block"
  },
  "jinja extends": {
    "prefix": "je",
    "body": [
      "{% extends '${1:base.html}' %}"
    ],
    "description": "jinja extends"
  },
  "jinja token": {
    "prefix": "jtoken",
    "body": [
      "{% csrf_token %}"
    ],
    "description": "jinja token"
  },
  "jinja url": {
    "prefix": "ju",
    "body": [
      "{% url '$1' %}"
    ],
    "description": "jinja url"
  }
}