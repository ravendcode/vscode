{
/*
	// Place your snippets for Python here. Each snippet is defined under a snippet name and has a prefix, body and
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
	// same ids are connected.
	// Example:
	"Print to console": {
		"prefix": "log",
		"body": [
			"console.log('$1');",
			"$2"
		],
		"description": "Log output to console"
	}
*/
	"Print to console": {
		"prefix": "pp",
		"body": [
			"print($1)"
		],
		"description": "Log output to console"
	},
	"Print to console alias": {
		"prefix": "cw",
		"body": [
			"print($1)"
		],
		"description": "Log output to console alias"
	},
	"from import": {
		"prefix": "ii",
		"body": [
			"from $1 import $2"
		],
		"description": "from import"
	},
	"from import one": {
		"prefix": "iii",
		"body": [
			"from $1 import $1"
		],
		"description": "from import one"
	},
	"def function": {
		"prefix": "deff",
		"body": [
			"def ${1:function}($2):",
			"\t${3:pass}"
		],
		"description": "def function"
	},
	"def function method": {
		"prefix": "defs",
		"body": [
			"def ${1:__init__}(self$2):",
			"\t${3:pass}"
		],
		"description": "def function method"
	},
	"def function with request": {
		"prefix": "defr",
		"body": [
			"def ${1:function}(request$2):",
			"\t${3:pass}"
		],
		"description": "def function with request"
	},
	"class object": {
		"prefix": "cc",
		"body": [
			"class ${1:Name}:",
			"\t${2:pass}"
		],
		"description": "class object"
	},
	"class model": {
		"prefix": "cm",
		"body": [
			"class ${1:Name}(${2:models.Model}):",
			"\t${3:pass}"
		],
		"description": "class model"
	},
	"class form": {
		"prefix": "cf",
		"body": [
			"class ${1:Name}Form(forms.${2:ModelForm}):",
			"\t${3:pass}"
		],
		"description": "class form"
	},
	"return": {
		"prefix": "rr",
		"body": [
			"return"
		],
		"description": "return"
	},
	"main": {
		"prefix": "main",
		"body": [
			"if __name__ == '__main__':",
			"\t${1:pass}"
		],
		"description": "main"
	}
}